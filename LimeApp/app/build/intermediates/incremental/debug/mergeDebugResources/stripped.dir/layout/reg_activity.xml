<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#151718"
    android:orientation="vertical"
    android:screenOrientation="landscape">

    <EditText
        android:id="@+id/PIB"
        android:layout_width="304.79dp"
        android:layout_height="43.67dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/inter"
        android:hint=""
        android:inputType="text"

        android:text=""
        android:textColor="#80F988"
        android:textColorHint="#545F71"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/telephon"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Прізвище та ім'я"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/PIB"
        app:layout_constraintEnd_toEndOf="@+id/PIB"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/PIB"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.986" />

    <EditText
        android:id="@+id/EditTextAge"
        android:layout_width="304.79dp"
        android:layout_height="43.67dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/inter"
        android:hint=""
        android:inputType="date"
        android:text=""

        android:textColor="#80F988"
        android:textColorHint="#545F71"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PIB" />

    <TextView

        android:layout_width="0dp"
        android:layout_height="19dp"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Дата народження"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextAge"
        app:layout_constraintEnd_toEndOf="@+id/EditTextAge"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/EditTextAge"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.986" />

    <EditText
        android:id="@+id/EditTextPhone"
        android:layout_width="304.79dp"
        android:layout_height="43.67dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/inter"
        android:hint=""
        android:inputType="phone"

        android:text=""
        android:textColor="#80F988"
        android:textColorHint="#545F71"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditTextAge" />

    <TextView

        android:layout_width="0dp"
        android:layout_height="19dp"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Телефон"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextPhone"
        app:layout_constraintEnd_toEndOf="@+id/EditTextPhone"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/EditTextPhone"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.986" />

    <EditText
        android:id="@+id/EditTextPass"
        android:layout_width="304.79dp"
        android:layout_height="43.67dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/inter"
        android:hint=""
        android:inputType="numberPassword"
        android:text=""

        android:textColor="#80F988"
        android:textColorHint="#545F71"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditTextPhone" />
    <TextView
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Пароль"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextPass"
        app:layout_constraintEnd_toEndOf="@+id/EditTextPass"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/EditTextPass"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.986" />

    <EditText
        android:id="@+id/EditTextPassRep"
        android:layout_width="304.79dp"
        android:layout_height="43.67dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/inter"
        android:hint=""
        android:inputType="numberPassword"
        android:text=""

        android:textColor="#80F988"
        android:textColorHint="#545F71"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditTextPass" />
    <TextView

        android:layout_width="0dp"
        android:layout_height="19dp"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Підтвердіть ваш пароль"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextPassRep"
        app:layout_constraintEnd_toEndOf="@+id/EditTextPassRep"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/EditTextPassRep"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.986" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.088"
        app:srcCompat="@drawable/sign_up" />

    <ImageView
        android:id="@+id/textButLog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditTextPassRep"
        app:srcCompat="@drawable/to_login_from_sing_up" />

    <ImageView
        android:id="@+id/button_reg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditTextPassRep"
        app:layout_constraintVertical_bias="0.774"
        app:srcCompat="@drawable/button" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_reg"
        app:layout_constraintEnd_toEndOf="@+id/button_reg"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/button_reg"
        app:layout_constraintTop_toTopOf="@+id/button_reg"
        app:layout_constraintVertical_bias="0.62"
        app:srcCompat="@drawable/iconofbuton" />


</androidx.constraintlayout.widget.ConstraintLayout>