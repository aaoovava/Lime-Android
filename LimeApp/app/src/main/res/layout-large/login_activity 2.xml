<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#151718"
    android:orientation="vertical"
    android:screenOrientation="landscape"
    tools:context=".Activitys.LogActivity">

    <EditText
        android:id="@+id/EditTextAge"
        android:layout_width="441.85dp"
        android:layout_height="63.37dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:fontFamily="@font/inter"
        android:hint=""
        android:inputType="phone"
        android:text=""
        android:textColor="#80F988"

        android:textColorHint="#545F71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.583" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="441.85dp"
        android:layout_height="63.37dp"
        android:background="@drawable/new_edit_text"
        android:drawableLeft="@drawable/rectangle_7oki"
        android:drawablePadding="20dp"
        android:ems="10"
        android:hint=""
        android:inputType="numberPassword"
        android:text=""
        android:textColor="#80F988"
        android:textColorHint="#545F71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.674" />

    <ProgressBar
        android:id="@+id/progressBar"

        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:progressTint="#7FF887"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        app:layout_constraintVertical_bias="0.317" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="682dp"
        android:layout_height="416dp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextAge"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.415"
        app:srcCompat="@drawable/mainlogtext" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="340dp"
        android:layout_height="136dp"
        app:layout_constraintBottom_toTopOf="@+id/telephon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.511"
        app:srcCompat="@drawable/regtext" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.90999997"
        app:srcCompat="@drawable/buttontomainview" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.62"
        app:srcCompat="@drawable/iconofbuton" />


    <TextView
        android:id="@+id/telephon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Телефон"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:textSize="20dp"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/EditTextAge"
        app:layout_constraintEnd_toEndOf="@+id/EditTextAge"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/EditTextAge"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.986" />

    <TextView
        android:id="@+id/telephon2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:gravity="top"
        android:lineSpacingExtra="4sp"
        android:text="Пароль"
        android:textAllCaps="false"
        android:textAppearance="@style/telephon"
        android:textSize="20dp"
        android:translationY="-1.92sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName2"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPersonName2"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName2"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.985" />

    <ImageView
        android:id="@+id/textButReg"
        android:layout_width="172dp"
        android:layout_height="58dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        app:layout_constraintVertical_bias="0.06999999"
        app:srcCompat="@drawable/uploudpass" />

</androidx.constraintlayout.widget.ConstraintLayout>

